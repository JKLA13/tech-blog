 {{#if user.posts.length}}
 <section class="post">
<div class="card mb-5">
   
    <h1>Blog Post History</h1>
    <h3>Posted by {{user.username}}</h3>
    {{#each user.posts as |post|}}
    <div>
    {{> post-details }}
    </div>
    {{/each}}
</div>
{{/if}}

</section>
<div>
    <button class="w-100 btn btn-md btn-primary m-1" id="new-blog-button">Add Tech Post</button>
</div>

<script src="/js/dashboard.js"></script>